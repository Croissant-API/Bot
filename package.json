{
  "name": "croissant-bot",
  "version": "1.0.0",
  "description": "template",
  "main": "src/index.ts",
  "scripts": {
    "start": "npx node dist/index.js",
    "lint": "npx eslint src --ext .ts",
    "lint:fix": "npx eslint src --ext .ts --fix",
    "format": "npx prettier --write src",
    "format:check": "npx prettier --check src",
    "dev": "npx ts-node-dev src/index.ts",
    "build": "npx tsc",
    "test": "node --experimental-vm-modules ./node_modules/jest-cli/bin/jest.js --detectOpenHandles --forceExit",
    "test-watch": "node --experimental-vm-modules ./node_modules/jest-cli/bin/jest.js --watch --detectOpenHandles  --forceExit"
  },
  "dependencies": {
    "croissant-bot": "file:",
    "discord.js": "^14.18.0",
    "dotenv": "^16.5.0",
    "node-fetch": "^2.7.0",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@types/express": "^5.0.1",
    "@types/node": "^14.0.0",
    "@types/node-fetch": "^2.6.12",
    "@types/yup": "^0.32.0",
    "eslint": "^9.25.0",
    "prettier": "^3.5.3",
    "supertest": "^7.1.0",
    "ts-node": "^10.0.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.30.1"
  },
  "author": "fox3000foxy",
  "license": "MIT"
}
